  "keywords": [
    "react"
  ],
  "files": [
    "lib",
    "src"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "dox": "^0.9.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jsdom": "^11.6.2",
    "nodeunit": "^0.11.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0"
  },
  "scripts": {
    "test": "nodeunit src/**_spec.js",
    "lint": "eslint src/**",
    "lint:fix": "eslint --fix src/**",
    "build": "npx babel src --out-dir lib",
    "build:examples": "npx babel-node examples",
    "build:docs": "cat src/navigators.js src/navigators-meta.js | dox -a > ./DOCS.md",
    "prepare": "npm run lint && npm run test && npm run build && npm run build:examples && npm run build:docs"
